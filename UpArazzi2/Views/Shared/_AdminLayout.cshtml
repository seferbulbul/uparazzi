    <script>
        $(document).ready(function () {
            $("#TeklifGonder2").click(function () {
                var id = $("#GonderilecekPortfoy").html();
                var kime = $("#teklifmail").val();

                if (!kime.includes("@@") || kime.length < 7) {
                    swal("Lütfen Geçerli Bir Mail Adresi Yazınız !", "", "warning");
                } else {
                    $.ajax({
                        type: "get",
                        url: "/Home/TeklifGonder",
                        data: { id: id, kime: kime },
                        success: function () {
                            $("#teklifmail").val("");
                            $(".close").click();
                            swal("", "Teklif Mailiniz Başarıyla Gönderilmiştir.", "success");
                        }, error: function () {
                            swal("", "Teklif Mailiniz Gönderilememiştir, Lütfen Daha Sonra Tekrar Deneyiniz.", "warning");
                        }
                    })
                }

            })
        })

        $(document).ready(function () {
            $("#destek").click(function () {
                var text = $("#message").val();
                var kime = $("#kime").val();
                if (text.trim() == "") {
                    swal("Lütfen Mesajınızı Yazınız", "", "warning");
                } else {
                    $.ajax({
                        type: "get",
                        url: "/Home/DestekTalebiGonder",
                        data: { text: text, kime: kime },
                        success: function () {
                            $("#destekkapat").click();
                            swal("", "Destek Talebiniz alınmıştır, En kısa sürede geri dönüş sağlacaktır.", "success");
                        }, error: function () {
                            swal("", "Destek Talebiniz gönderilememiştir, Lütfen Destek Birimiyle İletişime Geçiniz.", "warning");
                        }
                    })
                }

            })
        })

        $(document).ready(function () {
            $("#MailGonder2").click(function () {
                var kime = $("#GonderilecekKisi").html();
                var mesaj = $("#mesaj").val();

                if (mesaj.trim() == "") {
                    swal("Lütfen Mesajınızı Yazınız !", "", "warning");
                } else {
                    $.ajax({
                        type: "get",
                        url: "/Home/SendMessage",
                        data: { mesaj: mesaj, kime: kime },
                        success: function () {
                            $("#mesaj").val("");
                            $(".close").click();
                            swal("", "Mailiniz Başarıyla Gönderilmiştir.", "success");
                        }, error: function () {
                            swal("", "Mailiniz Gönderilememiştir, Lütfen Daha Sonra Tekrar Deneyiniz.", "warning");
                        }
                    })
                }

            })
        })
    </script>
